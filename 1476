#include <bits/stdc++.h>
int arr[101][101];
int main(){
	int n, m, t = 1;
	scanf("%d %d", &n, &m);
	for(int k=0; k<n+m-1; k++){
		for(int j=0; j<m; j++){
			for(int i=0; i<n; i++){
				if(i+j == k){
					arr[i][j] = t++;
					break;
				}
			}
		}
	}
	for(int i=0; i<n; i++, printf("\n")){
		for(int j=0; j<m; j++, printf(" ")){
			printf("%d", arr[i][j]);
		}
	}
}
